a <- {
'0':3,
'1':2,
'2',4,
'3',5,
'4',1,
length:5
}

轮数 = 1

while 轮数<a[length]
    左手指向的下标 = 0
    while 左手指向的下标 <= a[length]-1-轮数
        if a[左手指向的下标] > a[左手指向的下标+1])
            // 交换 左右手指向的下标位置
            t = a[左手指向的下标]
            a[左手指向的下标] = a[左手指向的下标+1]
            a[左手指向的下标] = t
        else
            // 什么也不做
        end
        左手指向的下标 <- 左手指向的下标+1
    end
    轮数 <- 轮数+1
end

print a


