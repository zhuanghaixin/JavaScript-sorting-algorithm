a <- {
'0':3,
'1':2,
'2',4,
'3',5,
'4',1,
length:5
}

轮数 <- 0

while 轮数<a[length]-1
    最小值下标 <- 轮数
    左手指向的下标 <- 轮数
    while 左手指向的下标 <= a[length]
        if a[左手指向的下标] < a[最小值下标]
            最小值下标 <- 左手指向的下标
        else
            // 什么也不做
        end
        左手指向的下标 <- 左手指向的下标+1
     end
    if 轮数！==最小值下标
    t <- a[轮数]
    a[轮数] <-a [最小值下标]
    a[最小值下标] <- t
    end
    轮数 <- 轮数+1
end

print a
